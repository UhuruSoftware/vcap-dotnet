<uhuru xmlns="urn:Uhuru.Configuration">
  <dea
    index="0"
    
    baseDir="c:\droplets"
    localRoute="8.8.8.8"
    filerPort="12345"
    statusPort="0"
    messageBus="nats://nats:nats@10.0.7.122:4222"
    
    multiTenant="true"
    maxMemoryMB="8000"
    secure="true"
    enforceUlimit="true"
    
    heartbeatIntervalMs="10000"
    advertiseIntervalMs="5000"
    
    useDiskQuota="true"
    maxConcurrentStarts="3">
    
    <directoryServer v2Port="34567" streamingTimeoutMS="60000" />

    <staging 
      enabled="false" 
      buildpacksDirectory="I:\_code\buildpacks"
    />
    
    <stacks>
      <stack name="windows2012"/>
      <!--<stack name="iis8"/>-->
    </stacks>
    
  </dea>
</uhuru>